{
    "welcomeFile": "/index.html",
    "authenticationMethod": "none",
    "logout": {
      "logoutEndpoint": "/do/logout"
    },
    "routes": [
      {
        "source": "^/index.html$",
        "localDir": "../resources"
      },
      {
        "source": "^/appconfig/(.*)$",
        "target": "/appconfig/$1",
        "localDir": "../resources"
      },
      {
        "source": "^/test-resources/(.*)$",
        "destination": "ui5"
      },
      {
        "source": "^/resources/(.*)$",
        "destination": "ui5"
      },
      {
        "source": "^/catalog/(.*)$",
        "target": "/catalog/$1",
        "destination": "susaas-srv-api"
      },
      {
        "source": "^(.*)$",
        "target": "$1",
        "service": "html5-apps-repo-rt"
      }
    ]
  }